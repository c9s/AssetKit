{
    "name": "corneltek/assetkit",
    "description": "High performance asset manager.",
    "keywords": [
        "asset",
        "manager",
        "javascript",
        "compress",
        "squash",
        "cssmin"
    ],
    "require": {
        "php": ">=5.4.0",
        "twig/twig": "^1 >=1.21",
        "corneltek/universal-cache": "~1",
        "corneltek/fileutil": "~1",
        "corneltek/cliframework": "~2",
        "corneltek/configkit": "*",
        "natxet/cssmin": "*",
        "symfony/process": "~2"
    },
    "require-dev": {
        "corneltek/phpunit-testmore": "dev-master",
        "satooshi/php-coveralls": "dev-master"
    },
    "bin": [
        "bin/assetkit"
    ],
    "autoload": {
        "psr-4": {
            "AssetKit\\": "src/AssetKit/"
        }
    },
    "extra": {
        "branch-alias": { "dev-master": "3.4.x-dev" }
    }
}
